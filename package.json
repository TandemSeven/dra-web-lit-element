{
  "name": "dra-web-lit-element",
  "version": "0.0.0",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "npm run serve | npm run watch",
    "serve": "polymer serve --port 8081",
    "watch": "browser-sync start --proxy localhost:8081 --open --files \"src/**/*.js\"",
    "build": "polymer build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/lit-element": "^0.6.4",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "2.2.1",
    "pwa-helpers": "^0.9.0"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "polymer-cli": "^1.9.2",
    "polyserve": "^0.27.0"
  }
}
